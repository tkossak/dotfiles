alias vbox='sudo mount -t vboxsf vbox_shared /mnt/vs && cd /mnt/vs'
alias vboxd='sudo mount -t vboxsf vbox_shared /mnt/vs && cd /mnt/vs/dotfiles'

alias lsblkl='sudo lsblk -fo "name,size,type,fstype,label,mountpoint,uuid"'
