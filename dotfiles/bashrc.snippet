alias ta="tmux attach-session"
alias tl='tmux list-sessions'
alias tn="tmux new-session"
alias r="ranger"
alias l="exa -l --git"
alias rg="rg -i"
alias ..='cd ..'
alias ...='cd ../..'
alias bwu='export BW_SESSION="$(bw unlock --raw)"'


alias asdfi='asdf install python'
alias asdfs='$ASDF_PYTHON_VERSION ='
